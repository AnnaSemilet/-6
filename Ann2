#include "stdafx.h"
#include "iostream" 

using namespace std;


int _tmain(int argc, _TCHAR* argv[])
{
	double k, n = 10, y ;
	cout << "Enter k:";
	cin >> k;
	cout << "Enter y:";
	cin >> y;
	int sum = 0;
	float rez;
	for (int k = 1; k < 11; k++);
	{
		sum = sum + k;
	}
	rez = log(sum + (k)) + pow(y, 5);
	cout << "rez= " << rez << endl;
	system("pause");
	return 0;
}
